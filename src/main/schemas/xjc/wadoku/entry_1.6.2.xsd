            </xs:enumeration>
            <xs:enumeration value="2e">
                <xs:annotation>
                    <xs:documentation>Shimo-Nidan</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="4">
                <xs:annotation>
                    <xs:documentation>Yondan</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="5">
                <xs:annotation>
                    <xs:documentation>Godan</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="suru">
                <xs:annotation>
                    <xs:documentation>sa-Hen</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="kuru">
                <xs:annotation>
                    <xs:documentation>ka-Hen</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="ra">
