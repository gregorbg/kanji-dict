                    <xs:documentation>Adv. mit &lt;Transcr.: to&gt;</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="taru" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Adv. mit &lt;Transcr.: to&gt; und Adn. mit &lt;Transcr.: taru&gt;</xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="suru" type="wd:transitivityEnum"/>
        </xs:complexType>
    </xs:element>

