    <xs:simpleType name="transitivityEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="trans">
                <xs:annotation>
                    <xs:documentation>他動詞</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="intrans">
                <xs:annotation>
                    <xs:documentation>自動詞</xs:documentation>
