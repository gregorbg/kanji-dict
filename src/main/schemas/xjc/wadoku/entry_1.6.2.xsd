<?xml version="1.0" encoding="UTF-8"?>
<xs:schema jaxb:version="2.1" elementFormDefault="qualified"
           targetNamespace="http://www.wadoku.de/xml/entry"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
           xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
           xmlns:wd="http://www.wadoku.de/xml/entry"
           xmlns:annox="http://annox.dev.java.net"
           jaxb:extensionBindingPrefixes="xjc annox"
           xmlns:xsl="http://www.w3.org/2001/XMLSchema">

    <xs:annotation>
        <xs:appinfo>
            <jaxb:globalBindings generateIsSetMethod="true">
                <jaxb:serializable uid="4815162342" />
                <!--<xjc:superClass name="de.wadoku.entry.EntryBase" /> -->
                <!--<xjc:simple /><!- - gibt es erst in JAXB 2.1 http://weblogs.java.net/blog/kohsuke/archive/2006/03/why_does_jaxb_p.html -->
            </jaxb:globalBindings>
        </xs:appinfo>
    </xs:annotation>

    <xs:element name="entry" >
        <xs:annotation>
            <xs:appinfo>
                <jaxb:class name="JaxbEntry" />
                <!-- <jaxb:class name="JaxbEntry" implClass="de.wadoku.entry.Entry" /> -->
                <!-- <jaxb:class ref="de.wadoku.entry.Entry" /> -->
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="wd:form" maxOccurs="1" minOccurs="1"/>
                <xs:element ref="wd:etym" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:gramGrp" minOccurs="0"/>
                <xs:element ref="wd:usg" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:sense" minOccurs="1" maxOccurs="unbounded" />
                <xs:element ref="wd:expl" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:count" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:ref" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:sref" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:link" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:steinhaus" minOccurs="0" maxOccurs="unbounded" />
                <xs:element ref="wd:wikide" maxOccurs="1" minOccurs="0"/>
                <xs:element ref="wd:wikija" maxOccurs="1" minOccurs="0"/>
                <xs:element ref="wd:ruigos" minOccurs="0" maxOccurs="1"/>
            </xs:sequence>
            <xs:attribute name="id" type="xs:long" default="0"/>
            <!-- // TODO Bei XML-Änderung Version ändern -->
            <xs:attribute name="version" type="wd:versionEnum" default="1.6">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <h2>Versionierungsformat x.y.z</h2>
                        <ul>
                            <li>Majorversion x bei grundlegenden strukturellen Änderungen.</li>
                            <li>Minorversion y bei inkompatiblen Änderungen.</li>
                            <li>(optional) Patchversion z bei einfachen Ergänzungen.</li>
                        </ul>
                        <h2>Änderungen</h2>
                        <h3>x.y (planned)</h3>
                        <ul>
                            <li>gramGrp-basierte POS-Angaben in DescrType</li>
                            <li>Flexionsangaben für Hilfsverben</li>
                            <li>SeasonPart 'ear' (Ähre, 穂), 'Kolbenwatte' (Kolbenwatte, 穂綿), vgl. 蒲 (ガマ)</li>
                            <li>Entfernen einiger obsoleter Elemente aus bracketType</li>
                        </ul>
                        <h3>1.6.2</h3>
                        <ul>
                            <li>Setsuzokujoshi</li>
                        </ul>
                        <h3>1.6.1</h3>
                        <ul>
                            <li>neu: foreign im defType</li>
                        </ul>
                        <h3>1.6.0</h3>
                        <ul>
                            <li>Entfernung ungenutzter Elemente in bracketType</li>
                            <li>Entfernung des Attributs wasInBracket</li>
                            <li>Entfernung des Attributs dicLevel</li>
                            <li>Entfernung ungenutzter Elemente in trComplexType</li>
                        </ul>
                        <h3>1.5.6</h3>
                        <ul>
                            <li>abbrevType für Etym</li>
                            <li>langEnum-Erweiterung</li>
                        </ul>
                        <h3>1.5.5</h3>
                        <ul>
                            <li>neuer saseru subEntryTypeEnum-Typ</li>
                        </ul>
                        <h3>1.5.4</h3>
                        <ul>
                            <li>Kakujoshi</li>
                        </ul>
                        <h3>1.5.3</h3>
                        <ul>
                            <li>katsuyouType, primär für Jodoushi</li>
                        </ul>
                        <h3>1.5.2</h3>
                        <ul>
                            <li>neuer sg NumerusEnum-Typ</li>
                        </ul>
                        <h3>1.5.1</h3>
                        <ul>
                            <li>neuer mnf GenusEnum-Typ</li>
                        </ul>
                        <h3>1.5</h3>
                        <ul>
                            <li>Bereinigung von BracketType</li>
                            <li>orthIrrEnum -> orthTypeEnum</li>
                            <li>orth/@irr -> orth/@type</li>
                            <li>orthTypeEnum: delete 'kanji', add 'mistake', 'true' -> 'irreg'</li>
                        </ul>
                        <h3>1.4.1</h3>
                        <ul>
                            <li>shiku-Attribut für Keiyoushi</li>
                            <li>SeasonPart 'bud'</li>
                        </ul>
                        <h3>1.4</h3>
                        <ul>
                            <li>erweiterte Season-Angaben -> seasonword, @season veraltet</li>
                            <li>aufgedröseltes Sense-Element</li>
                            <li>Scientif im Def-Element</li>
                            <li>Senseid-Attribut im Sense-Element sowie im Ref-Element</li>
                            <li>Entfernen des verworfenen OrthType contents</li>
                            <li>Hinzufügen der Ruigos/Ruigo-Elemente</li>
                        </ul>
                        <h3>1.3</h3>
                        <ul>
                            <li>strukturierte POS-Angaben</li>
                            <li>pron veraltet, ersetzt durch prons</li>
                            <li>globales expl element</li>
                        </ul>
                        <h3>1.2</h3>
                        <ul>
                            <li>Unterstützung für Subsenses und descr-Element.</li>
                            <li>Attribut wasInBracket für trans[@langdesc='scientific'], ob es in Klammern stand.</li>
                            <li>Entry-Attribut HE = Haupteintrag</li>
                            <li>Pron-Attribute altaccent = alternative Akzent-Angabe</li>
                            <li>tr-Attribute jlpt/genki = Verweis auf Vokabellisten JLPT/Genki</li>
                            <li>veraltete Text-Elemente in Orth und Pron</li>
                        </ul>
                        <h3>1.1</h3>
                        <ul>
                            <li>erste Releaseversion.</li>
                        </ul>
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
            <xs:attribute name="HE" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        wenn <code>true</code>, dann Haupteintrag
                    </xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="versionEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="1.1"/>
            <xs:enumeration value="1.2"/>
            <xs:enumeration value="1.3"/>
            <xs:enumeration value="1.4"/>
            <xs:enumeration value="1.5"/>
            <xs:enumeration value="1.6"/>
         </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="seasonEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="summer"/>
            <xs:enumeration value="autumn"/>
            <xs:enumeration value="spring"/>
            <xs:enumeration value="winter"/>
            <xs:enumeration value="newyear"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="seasonPart">
        <xs:restriction base="xs:string">
            <xs:enumeration value="bud"/>
            <xs:enumeration value="flower"/>
            <xs:enumeration value="fruit"/>
            <xs:enumeration value="leaf"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="usgEnumBasic">
        <xs:restriction base="xs:string">
            <xs:enumeration value="obsol."/>
        </xs:restriction>
    </xs:simpleType>

    <!-- POS -->
    <xs:element name="katsuyou" type="wd:katsuyouType"/>
    <xs:element name="meishi">
        <xs:annotation>
            <xs:documentation>Nomen/名刺</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="suru" type="wd:transitivityEnum"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="doushi">
        <xs:annotation>
            <xs:documentation>Verb/動詞</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="level" type="wd:levelEnum"/>
            <xs:attribute name="transitivity" type="wd:transitivityEnum"/>
            <xs:attribute name="godanrow">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="wa"/>
                        <xs:enumeration value="wa_o">
                            <xs:annotation>
                                <xs:documentation>
                                    アワ行五
                                    連用形-ウ音便
                                    5‑st. trans. V. auf &lt;Transcr.: ‑[w]a&gt; mit u‑Onbin = &lt;Transcr.: ‑ō/ūte&gt;
                                </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="ba"/>
                        <xs:enumeration value="ga"/>
                        <xs:enumeration value="ka">
                            <xs:annotation>
                                <xs:documentation>
                                    連用形-イ音便
                                </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="ka_i_yu">
                            <xs:annotation>
                                <xs:documentation>
                                    連用形-促音便
                                    5‑st. intrans. V. auf &lt;Transcr.: ‑ka&gt; &lt;Jap.: 行く&gt; mit Geminaten-Onbin = &lt;Transcr.: ‑tte&gt;
                                </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="ra"/>
                        <xs:enumeration value="ra_i">
                            <xs:annotation>
                                <xs:documentation>
                                    ラ行五（四）段特別活用
                                    連用形-イ音便
                                    5‑st. trans. V. auf &lt;Transcr.: ‑ra&gt;, Sonderform mit Renyō·kei &lt;Transcr.: ‑i&gt;
                                </xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="na"/>
                        <xs:enumeration value="ma"/>
                        <xs:enumeration value="sa"/>
                        <xs:enumeration value="ta"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="onbin" type="xs:boolean"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="keiyoushi">
        <xs:annotation>
            <xs:documentation>i-Adjektiv/ku-Adjektiv/形容詞</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="ku" type="xs:boolean"/>
            <xs:attribute name="shiku" type="xs:boolean"/>
        </xs:complexType>
    </xs:element>

    <xs:element name="keiyoudoushi">
        <xs:annotation>
            <xs:documentation>na-Adjektiv/Nominaladjektiv/形容動詞</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="no" type="xs:boolean"/>
            <xs:attribute name="nari" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>Na.-Adj. mit &lt;Transcr.: nari&gt;</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:simpleType name="levelEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="1i">
                <xs:annotation>
                    <xs:documentation>Kami-Ichidan</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="1e">
                <xs:annotation>
                    <xs:documentation>Shimo-Ichidan</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="2i">
